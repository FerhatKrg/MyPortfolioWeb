@model Message
@{
    ViewData["Title"] = "MessageDetail";
    Layout = "~/Views/Layout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Mesajlar</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Mesaj Listesi</div>
					</div>
					<div class="card-body">
						<div class="card-sub">
							Gönderen : @Model.NameSurname
							<br />
							Konu: @Model.Subject
							<br />
							Mail: @Model.Email
							<br />
							Tarih: @Model.SendDate
							<br />
							<br />
							<br />
							<br />

							Mesaj: @Model.MessageDetail
						</div>
						

					</div>
				</div>
			</div>
		</div>
	</div>
</div>
